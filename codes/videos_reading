import os
import cv2

#read video 
video_path = os.path.join('..','resources','Computer_Vision.mp4')
video = cv2.VideoCapture(video_path)

# Visualize video 

ret = True
while  ret:
    ret,frame= video.read()
    if ret : 
        cv2.imshow('frame', frame)
        cv2.waitKey(34)
video.release()
cv2.destroyAllWindows()
